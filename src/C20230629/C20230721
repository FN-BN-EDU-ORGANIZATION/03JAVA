#include <stdio.h>
void main(){
  int a= 30, b= 15;
printf("%d%d", a & b, a | b);   비트 연산자 문제 띄워쓰기X
}

#include <stdio.h>
void main(){
char b[16] = "hello world";
printf("%s", &b[6]); 6번째 문자열 출력
}


i = 0
sum = 0
while i <10 :
i = i+ 1
if i % 3 == 0 :
 sum -= i
elif i % 3 == 1:
  sum += i
else:
  sum *= i

print(sum)  
